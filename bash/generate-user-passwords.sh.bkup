genchar (){
	code=$(bc <<< "${RANDOM}%94+33")
	char=$(echo $code | awk '{ printf("%c",$0); }')
	echo "${char}"
}

genpass (){
	length=16
	for char in $(seq 1 $length); do
	password+=$(genchar)	
	done
	echo "${password}"
}



userpass=''

for arg in $@; do
        userpass=$(genpass)
	echo "${arg}"
        echo "${userpass}"
done

